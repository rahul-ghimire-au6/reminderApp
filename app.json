{
  "expo": {
    "name": "reminderApp",
    "slug": "reminderApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/reminderIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/reminder_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "permissions": ["RECEIVE_BOOT_COMPLETED"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/reminderIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rahul.reminderApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins":[
      [
        "expo-notifications",
        {
          "sounds": ["./assets/nokia_03.wav","assets/nokia_03.wav"]
        }
      ]
    ]
  }
}
